# include<stdio.h>
int main(){
  int mark;
    printf("enter a marks secured by student");
    scanf("%d",&mark);
    if(mark>=90 && mark<=100){
      printf("Grade is A+\n");
    } else if (mark>=70 && mark<90){
      printf("Grade is A\n");
    } else if (mark>=30 && mark<70){
      printf("Grade is B");
    } else {
      printf("Grade is C");
    }
    return 0;
}
